<tars>
	<application>
		enableset=n
		setdivision=NULL
		<server>
       #     enable_php_server=1
			# node=tars.tarsnode.ServerObj@tcp -h 127.0.0.1 -p 19386 -t 60000
			deactivating-timeout=3000
			app=WinwinRpc
			server=EventBusServer
			localip=127.0.0.1
			local=tcp -h 127.0.0.1 -p 18081 -t 3000
			basepath=.
			datapath=./runtime/
			logpath=./runtime/
			loglevel=DEBUG
			logsize=15M
			log=tars.tarslog.LogObj
			config=tars.tarsconfig.ConfigObj
			notify=tars.tarsnotify.NotifyObj
			logLevel=DEBUG
			php=/usr/bin/php
			buffer_output_size=12582912
			open_tcp_nodelay=1
			open_eof_check=0
			open_eof_split=0
			task_worker_num=1
			dispatch_mode=2
			daemonize=0
			<WinwinRpc.EventBusServer.EventBusObjAdapter>
				allow
				endpoint=tcp -h 127.0.0.1 -p 18081 -t 60000
				handlegroup=WinwinRpc.EventBusServer.EventBusObjAdapter
				maxconns=200000
				protocol=tars
				queuecap=10000
				queuetimeout=60000
				servant=WinwinRpc.EventBusServer.EventBusObj
				threads=5
			</WinwinRpc.EventBusServer.EventBusObjAdapter>
			<WinwinRpc.EventBusServer.JobStatObjAdapter>
				allow
				endpoint=tcp -h 127.0.0.1 -p 18081 -t 60000
				handlegroup=WinwinRpc.EventBusServer.JobStatObjAdapter
				maxconns=200000
				protocol=tars
				queuecap=10000
				queuetimeout=60000
				servant=WinwinRpc.EventBusServer.JobStatObj
				threads=5
			</WinwinRpc.EventBusServer.JobStatObjAdapter>
		</server>
		<client>
			asyncthread=3
			locator=tars.tarsregistry.QueryObj@tcp -h 127.0.0.1 -p 17890
			sync-invoke-timeout=20000
			async-invoke-timeout=20000
			refresh-endpoint-interval=60000
			stat=tars.tarsstat.StatObj
			property=tars.tarsproperty.PropertyObj
			report-interval=60000
			modulename=WinwinRpc.EventBusServer
			sample-rate=100000
			max-sample-count=50
		</client>
	</application>
</tars>
